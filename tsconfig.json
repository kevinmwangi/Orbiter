{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "target": "ESNext",
    "module": "ESNext",
    "types": ["node", "@nuxt/types"]
  },
  "include": [
    "./**/*.ts",
    "./**/*.vue",
    "./types/**/*.d.ts",
    ".nuxt/nuxt.d.ts"
  ]
}