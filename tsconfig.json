{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "types": ["@nuxt/types", "@types/node"],
    "typeRoots": ["./types"]
  },
  "include": [
    "./**/*.ts",
    "./**/*.vue",
    "./types/**/*.d.ts"
  ]
}